{
    "name": "customer-gui-frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev-https": "node run-ssl-proxy",
        "prod": "yarn build && yarn start",
        "prod:serve": "yarn start",
        "build": "next build",
        "build:analyze": "ANALYZE=true yarn build",
        "start": "next start",
        "lint": "eslint ./src --ext .js,.ts,.tsx,.mdx --fix",
        "format": "prettier ./src --write --ignore-path .prettierignore",
        "tsc": "tsc",
        "tokens": "gds-tokens",
        "repack": "rm -rf ./node_modules && yarn cache clean -all && yarn",
        "prepare": "husky install"
    },
    "dependencies": {
        "@babel/plugin-transform-private-property-in-object": "7.24.5",
        "@greensight/gds": "3.0.14",
        "@next/bundle-analyzer": "14.2.3",
        "browserslist": "^4.23.0",
        "classnames": "2.5.1",
        "deepmerge": "4.3.1",
        "husky": "9.0.11",
        "lodash": "4.17.21",
        "next": "14.2.3",
        "next-compose-plugins": "^2.2.1",
        "next-plugin-svgr": "^1.1.10",
        "react": "^18.3.1",
        "react-dom": "18.3.1",
        "react-fast-compare": "3.2.2",
        "react-merge-refs": "2.1.1",
        "webpack": "^5.91.0"
    },
    "devDependencies": {
        "@babel/core": "^7.24.5",
        "@babel/helper-plugin-utils": "^7.24.5",
        "@babel/preset-react": "^7.24.1",
        "@svgr/webpack": "^8.1.0",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/react-dom": "18.3.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "babel-plugin-react-require": "4.0.2",
        "css-loader": "7.1.1",
        "eslint": "^8.57.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-next": "^14.2.3",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-mdx": "2.3.4",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.2",
        "eslint-plugin-storybook": "^0.8.0",
        "lint-staged": "15.2.2",
        "prettier": "3.2.5",
        "sass": "^1.77.2",
        "typescript": "5.4.5",
        "yarn-audit-fix": "^10.0.7"
    },
    "browserslist": [
        "last 2 versions",
        "not dead",
        "not IE 11"
    ],
    "lint-staged": {
        "*.{js,ts,tsx,mdx}": "eslint --cache --fix",
        "*.prettierignore": "prettier --write"
    }
}
